{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "allowJs": "true",
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ]
    }
  },
  "include": [
    "./run-server.js"
  ]
}
